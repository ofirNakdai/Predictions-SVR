<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="desktop.details.action.impl.ReplaceController">
   <children>
      <Label text="Replace" />
      <Separator />
      <HBox spacing="10.0">
         <children>
            <VBox>
               <children>
                  <Label text="Entity to kill" />
                  <Label text="Entity to create" />
                  <Label text="Secondary Entity" />
                  <Label text="Mode" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label fx:id="mainEntLabel" text="Label" />
                  <Label fx:id="createLabel" text="Label" />
                  <Label fx:id="secEntLabel" text="Label" />
                  <Label fx:id="modeLabel" text="Label" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
