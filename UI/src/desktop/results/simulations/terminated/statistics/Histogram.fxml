<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="desktop.results.simulations.terminated.statistics.HistogramController">
   <center>
      <ScrollPane BorderPane.alignment="CENTER">
         <content>
            <BarChart fx:id="barChart" styleClass="bar-chart" title="Entities Histogram">
              <xAxis>
                <CategoryAxis label="Property Value" side="BOTTOM" fx:id="xAxis" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="yAxis" label="Count" side="LEFT" />
              </yAxis>
            </BarChart>
         </content>
      </ScrollPane>
   </center>
</BorderPane>
